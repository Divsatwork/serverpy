{
    "service1": {
        "server": ["127.0.0.1:8080", "127.0.0.1:8081"],
        "polling": {
            "method": "ping"
        },
        "apis": [
            "/first", "/second/sample"
        ]
    },
    "service2": {
        "server": ["someurlhere.com"],
        "polling": {
            "method": "health",
